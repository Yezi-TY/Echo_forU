{
  "name": "music-gen-ui-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "DiffRhythm2 GUI Frontend Workspace",
  "scripts": {
    "dev": "pnpm --filter web dev & pnpm --filter desktop dev",
    "dev:web": "pnpm --filter web dev",
    "dev:desktop": "pnpm --filter desktop dev",
    "build": "pnpm --filter shared build && pnpm --filter web build && pnpm --filter desktop build",
    "build:web": "pnpm --filter web build",
    "build:desktop": "pnpm --filter desktop build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "type-check": "pnpm -r type-check"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}

